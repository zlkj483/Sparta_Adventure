void CalcPixelMaterialInputs(in out FMaterialPixelParameters Parameters, in out FPixelMaterialInputs PixelMaterialInputs)
{
	//WorldAligned texturing & others use normals & stuff that think Z is up
	Parameters.TangentToWorld[0] = Parameters.TangentToWorld[0].xzy;
	Parameters.TangentToWorld[1] = Parameters.TangentToWorld[1].xzy;
	Parameters.TangentToWorld[2] = Parameters.TangentToWorld[2].xzy;

	float3 WorldNormalCopy = Parameters.WorldNormal;

	// Initial calculations (required for Normal)

	// The Normal is a special case as it might have its own expressions and also be used to calculate other inputs, so perform the assignment here
	PixelMaterialInputs.Normal = MaterialFloat3(0.00000000,0.00000000,1.00000000);


#if TEMPLATE_USES_STRATA
	Parameters.SharedLocalBases = StrataInitialiseSharedLocalBases();
#endif

	// Note that here MaterialNormal can be in world space or tangent space
	float3 MaterialNormal = GetMaterialNormal(Parameters, PixelMaterialInputs);

#if MATERIAL_TANGENTSPACENORMAL
#if SIMPLE_FORWARD_SHADING
	Parameters.WorldNormal = float3(0, 0, 1);
#endif

#if FEATURE_LEVEL >= FEATURE_LEVEL_SM4
	// Mobile will rely on only the final normalize for performance
	MaterialNormal = normalize(MaterialNormal);
#endif

	// normalizing after the tangent space to world space conversion improves quality with sheared bases (UV layout to WS causes shrearing)
	// use full precision normalize to avoid overflows
	Parameters.WorldNormal = TransformTangentNormalToWorld(Parameters.TangentToWorld, MaterialNormal);

#else //MATERIAL_TANGENTSPACENORMAL

	Parameters.WorldNormal = normalize(MaterialNormal);

#endif //MATERIAL_TANGENTSPACENORMAL

#if MATERIAL_TANGENTSPACENORMAL
	// flip the normal for backfaces being rendered with a two-sided material
	Parameters.WorldNormal *= Parameters.TwoSidedSign;
#endif

	Parameters.ReflectionVector = ReflectionAboutCustomWorldNormal(Parameters, Parameters.WorldNormal, false);

#if !PARTICLE_SPRITE_FACTORY
	Parameters.Particle.MotionBlurFade = 1.0f;
#endif // !PARTICLE_SPRITE_FACTORY

	// Now the rest of the inputs
	MaterialFloat2 Local0 = Parameters.TexCoords[1].xy;
	MaterialFloat2 Local1 = (DERIV_BASE_VALUE(Local0) * ((MaterialFloat2)12.00000000));
	MaterialFloat Local2 = MaterialStoreTexCoordScale(Parameters, DERIV_BASE_VALUE(Local1), 1);
	MaterialFloat4 Local3 = ProcessMaterialColorTextureLookup(Texture2DSampleBias(Material_Texture2D_0,samplerMaterial_Texture2D_0,DERIV_BASE_VALUE(Local1),View.MaterialTextureMipBias));
	MaterialFloat Local4 = MaterialStoreTexSample(Parameters, Local3, 1);
	MaterialFloat3 Local5 = (Local3.rgb * ((MaterialFloat3)Material.PreshaderBuffer[2].x));
	MaterialFloat3 Local6 = (Local5 * ((MaterialFloat3)Material.PreshaderBuffer[2].y));
	MaterialFloat3 Local7 = (Material.PreshaderBuffer[3].xyz + Local6);
	MaterialFloat Local8 = dot(Parameters.CameraVector,MaterialFloat3(0.00000000,0.00000000,-1.00000000));
	MaterialFloat Local9 = saturate(Local8);
	MaterialFloat Local10 = (1.00000000 - Local9);
	MaterialFloat Local11 = PositiveClampedPow(Local10,Material.PreshaderBuffer[5].x);
	MaterialFloat Local12 = saturate(Local11);
	MaterialFloat3 Local13 = lerp(Local7,Material.PreshaderBuffer[5].yzw,Local12);
	MaterialFloat Local14 = dot(Parameters.CameraVector,Material.PreshaderBuffer[8].xyz);
	MaterialFloat Local15 = (Local14 - 1.00000000);
	MaterialFloat Local16 = abs(Local15);
	MaterialFloat Local17 = (Local16 * Material.PreshaderBuffer[9].y);
	MaterialFloat Local18 = (1.00000000 - Local17);
	MaterialFloat Local19 = (Local18 * 1.00000000);
	MaterialFloat Local20 = saturate(Local19);
	MaterialFloat3 Local21 = (((MaterialFloat3)Local20) * Material.PreshaderBuffer[12].xyz);
	MaterialFloat3 Local22 = (Local13 + Local21);
	MaterialFloat Local23 = (View.GameTime * Material.PreshaderBuffer[14].x);
	MaterialFloat Local24 = (Local23 * 0.00020000);
	MaterialFloat Local25 = (Local23 * 0.00000000);
	MaterialFloat2 Local26 = Parameters.TexCoords[0].xy;
	MaterialFloat2 Local27 = (MaterialFloat2(Local24,Local25) + DERIV_BASE_VALUE(Local26));
	MaterialFloat Local28 = MaterialStoreTexCoordScale(Parameters, DERIV_BASE_VALUE(Local27), 2);
	MaterialFloat4 Local29 = ProcessMaterialColorTextureLookup(Texture2DSampleBias(Material_Texture2D_1,samplerMaterial_Texture2D_1,DERIV_BASE_VALUE(Local27),View.MaterialTextureMipBias));
	MaterialFloat Local30 = MaterialStoreTexSample(Parameters, Local29, 2);
	MaterialFloat Local31 = (Local23 * 0.00100000);
	MaterialFloat2 Local32 = (MaterialFloat2(Local31,Local25) + DERIV_BASE_VALUE(Local26));
	MaterialFloat Local33 = MaterialStoreTexCoordScale(Parameters, DERIV_BASE_VALUE(Local32), 0);
	MaterialFloat4 Local34 = ProcessMaterialGreyscaleTextureLookup(Texture2DSampleBias(Material_Texture2D_2,samplerMaterial_Texture2D_2,DERIV_BASE_VALUE(Local32),View.MaterialTextureMipBias));
	MaterialFloat Local35 = MaterialStoreTexSample(Parameters, Local34, 0);
	MaterialFloat Local36 = lerp(Local29.r,Local34.r,Local9);
	FLWCVector3 Local37 = GetWorldPosition(Parameters);
	FLWCVector3 Local38 = LWCSubtract(DERIV_BASE_VALUE(Local37), GetObjectWorldPosition(Parameters));
	MaterialFloat Local39 = (GetPrimitiveData(Parameters).ObjectRadius * -0.10000000);
	FLWCVector3 Local40 = LWCDivide(DERIV_BASE_VALUE(Local38), ((MaterialFloat3)Local39));
	FLWCScalar Local41 = LWCGetZ(DERIV_BASE_VALUE(Local40));
	MaterialFloat Local42 = LWCSaturate(DERIV_BASE_VALUE(Local41));
	MaterialFloat Local43 = (1.00000000 - DERIV_BASE_VALUE(Local42));
	MaterialFloat Local44 = (DERIV_BASE_VALUE(Local43) * Material.PreshaderBuffer[14].y);
	MaterialFloat Local45 = lerp(0.00000000,Local36,DERIV_BASE_VALUE(Local44));
	MaterialFloat2 Local46 = (DERIV_BASE_VALUE(Local26) * ((MaterialFloat2)0.50000000));
	MaterialFloat Local47 = MaterialStoreTexCoordScale(Parameters, DERIV_BASE_VALUE(Local46), 0);
	MaterialFloat4 Local48 = ProcessMaterialGreyscaleTextureLookup(Texture2DSampleBias(Material_Texture2D_2,samplerMaterial_Texture2D_2,DERIV_BASE_VALUE(Local46),View.MaterialTextureMipBias));
	MaterialFloat Local49 = MaterialStoreTexSample(Parameters, Local48, 0);
	MaterialFloat Local50 = lerp(Material.PreshaderBuffer[14].w,Material.PreshaderBuffer[14].z,Local48.r);
	MaterialFloat Local51 = PositiveClampedPow(Local45,Local50);
	MaterialFloat3 Local52 = (Material.PreshaderBuffer[15].xyz * ((MaterialFloat3)Local51));
	MaterialFloat Local53 = (Local16 * (1.00000000 / max(0.00001000,1.29999995)));
	MaterialFloat Local54 = (1.00000000 - Local53);
	MaterialFloat Local55 = (Local54 * 1.00000000);
	MaterialFloat Local56 = saturate(Local55);
	MaterialFloat Local57 = PositiveClampedPow(Local56,10.00000000);
	MaterialFloat Local58 = saturate(Local57);
	MaterialFloat3 Local59 = (((MaterialFloat3)Local58) * Material.PreshaderBuffer[11].yzw);
	MaterialFloat Local60 = (Local51 * Local51);
	MaterialFloat Local61 = (Local60 * 0.40000001);
	MaterialFloat3 Local62 = (Local59 * ((MaterialFloat3)Local61));
	MaterialFloat3 Local63 = (Local52 + Local62);
	MaterialFloat Local64 = saturate(Local60);
	MaterialFloat3 Local65 = lerp(Local22,Local63,Local64);
	MaterialFloat3 Local66 = (Local65 * Material.PreshaderBuffer[17].xyz);
	MaterialFloat3 Local67 = (Local66 * ((MaterialFloat3)1.50000000));
	MaterialFloat3 Local68 = lerp(Local67,Material.PreshaderBuffer[18].xyz,Material.PreshaderBuffer[17].w);

	PixelMaterialInputs.EmissiveColor = Local68;
	PixelMaterialInputs.Opacity = 1.00000000;
	PixelMaterialInputs.OpacityMask = 1.00000000;
	PixelMaterialInputs.BaseColor = MaterialFloat3(0.00000000,0.00000000,0.00000000);
	PixelMaterialInputs.Metallic = 0.00000000;
	PixelMaterialInputs.Specular = 0.50000000;
	PixelMaterialInputs.Roughness = 0.50000000;
	PixelMaterialInputs.Anisotropy = 0.00000000;
	PixelMaterialInputs.Normal = MaterialFloat3(0.00000000,0.00000000,1.00000000);
	PixelMaterialInputs.Tangent = MaterialFloat3(1.00000000,0.00000000,0.00000000);
	PixelMaterialInputs.Subsurface = 0;
	PixelMaterialInputs.AmbientOcclusion = 1.00000000;
	PixelMaterialInputs.Refraction = 0;
	PixelMaterialInputs.PixelDepthOffset = 0.00000000;
	PixelMaterialInputs.ShadingModel = 0;
	PixelMaterialInputs.FrontMaterial = GetInitialisedStrataData();


#if MATERIAL_USES_ANISOTROPY
	Parameters.WorldTangent = CalculateAnisotropyTangent(Parameters, PixelMaterialInputs);
#else
	Parameters.WorldTangent = 0;
#endif
}

